{
  "name": "eribertolopez-backend",
  "version": "1.0.0",
  "description": "Backend services for Eriberto Lopez Portfolio",
  "main": "index.js",
  "author": "elopez",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "build": "rspack build",
    "watch": "rspack --watch --mode development",
    "prebuild": "rm -rf dist",
    "migration:run": "typeorm migration:run -d ./src/lib/database/database.ts",
    "migration:create": "typeorm migration:create ./src/lib/database/migrations/$FILE_NAME",
    "migration:revert": "typeorm migration:revert",
    "migration:generate": "typeorm migration:generate -d ./src/lib/database/database.ts"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.823.0",
    "@aws-sdk/client-dynamodb": "^3.749.0",
    "@aws-sdk/client-ecs": "^3.798.0",
    "@aws-sdk/client-eventbridge": "^3.758.0",
    "@aws-sdk/client-lambda": "^3.799.0",
    "@aws-sdk/client-s3": "^3.749.0",
    "@aws-sdk/client-scheduler": "^3.758.0",
    "@aws-sdk/client-sqs": "^3.799.0",
    "@aws-sdk/client-ssm": "^3.759.0",
    "@aws-sdk/lib-dynamodb": "^3.749.0",
    "@aws-sdk/s3-request-presigner": "^3.749.0",
    "aws-jwt-verify": "^5.0.0",
    "aws-lambda": "^1.0.7",
    "date-fns": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.14.0",
    "pg-promise": "^11.11.0",
    "redis": "^4.7.1",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.5.1",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rspack/cli": "^1.4.11",
    "@types/aws-lambda": "^8.10.147",
    "@types/date-fns": "^2.6.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.11.6",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.4",
    "aws-cdk-lib": "^2.210.0",
    "constructs": "^10.4.2",
    "eslint": "^8.57.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typeorm-fixtures-cli": "^4.1.0",
    "typescript": "^5.9.2"
  }
}
